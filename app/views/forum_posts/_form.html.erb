<%= form_for @forum_post do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :content %><br />
    <%= f.text_area :content %>
  </p>
  <p>
    <%= f.label :user_id %><br />
    <%= f.text_field :user_id %>
  </p>
  <p>
    <%= f.label :thread_id %><br />
    <%= f.text_field :thread_id %>
  </p>
  <p><%= f.submit %></p>
<% end %>
