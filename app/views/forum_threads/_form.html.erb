<%= form_for @forum_thread do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :title %><br />
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.label :description %><br />
    <%= f.text_field :description %>
  </p>
  <p>
    <%= f.label :user_id %><br />
    <%= f.text_field :user_id %>
  </p>
  <p>
    <%= f.label :catagory_id %><br />
    <%= f.text_field :catagory_id %>
  </p>
  <p><%= f.submit %></p>
<% end %>
